{
  "source": {
    "include": [ "src/js/" ],
    "includePattern": ".js$"
  },
  "opts": {
    "destination": "docs/api",
    "readme": "docs/index.md",
    "template": "node_modules/tui-jsdoc-template",
    "recurse": true,
    "tutorials": "docs/guides",
    "encoding": "utf8"
  },
  "templates": {
    "logo": {
      "url": "//videojs.com/img/logo.png",
      "height": "30px",
      "width": "214px"
    },
    "name": "Video.js Documentation"
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "tags": ["example"]
  }
}
